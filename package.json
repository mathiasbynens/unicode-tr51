{
  "name": "unicode-tr51",
  "version": "8.0.1",
  "description": "Emoji data extracted from Unicode Technical Report #51.",
  "homepage": "https://mths.be/unicode-tr51",
  "main": "symbols.js",
  "keywords": [
    "unicode",
    "unicode-data",
    "code points",
    "symbols",
    "characters",
    "emoji",
    "data"
  ],
  "license": "MIT",
  "author": {
    "name": "Mathias Bynens",
    "url": "https://mathiasbynens.be/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mathiasbynens/unicode-tr51.git"
  },
  "bugs": "https://github.com/mathiasbynens/unicode-tr51/issues",
  "files": [
    "LICENSE-MIT.txt",
    "index.js",
    "Emoji.js",
    "Emoji_Component.js",
    "Emoji_Modifier.js",
    "Emoji_Modifier_Base.js",
    "Emoji_Presentation.js"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "download": "curl -L http://unicode.org/Public/emoji/5.0/emoji-data.txt > data/emoji-data.txt",
    "build": "node scripts/parse-emoji-data.js",
    "coverage": "istanbul cover --report html node_modules/.bin/_mocha scripts/parse-emoji-data.js -- -u exports -R spec",
    "test": "mocha tests"
  },
  "devDependencies": {
    "coveralls": "^2.11.1",
    "istanbul": "^0.4.5",
    "jsesc": "^2.2.0",
    "mocha": "^3.1.2"
  }
}
